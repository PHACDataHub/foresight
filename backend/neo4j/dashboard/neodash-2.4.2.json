          {
            "id": "0827c67b-a42c-482a-92d5-3dce7c9542fc",
            "title": "Clusters with detected threats",
            "query": "WITH CASE WHEN $neodash_end_date > $neodash_start_date THEN $neodash_start_date + '-' + $neodash_end_date ELSE $neodash_start_date END AS period\nWITH period + '-\\\\d+' AS id_pattern\n  MATCH (c:Cluster)-[r:DETECTED_THREAT]->(t:Threat)\n    WHERE c.id =~ id_pattern\n      AND t.text IN $neodash_threat_text\nRETURN c, r, t\n\n\n\n\n\n\n",
